{
  "name": "next-query-utils",
  "version": "1.1.0",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "description": "Provides functions to manipulate parsed url query object",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm-run-all -s setup:*",
    "setup:simple-git-hooks": "simple-git-hooks",
    "test": "jest",
    "watch": "parcel watch",
    "check": "tsc --noEmit",
    "build": "parcel build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --options typedoc.json",
    "prepack": "npm-run-all -s check build"
  },
  "browserslist": [
    "> 0.5% and not dead",
    "IE 11"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "nextjs",
    "url",
    "utilities",
    "utils"
  ],
  "author": "honey32",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:honey32/next-query-utils.git"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.9.3",
    "prettier": "^3.1.1",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.6",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.1"
  },
  "volta": {
    "node": "20.10.0"
  }
}
